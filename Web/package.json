{
  "name": "component-arkit-web",
  "version": "0.0.0",
  "description": "component-arkit-web",
  "scripts": {
    "start": "yarn run build:clean; concurrently 'yarn run assets' 'yarn run server' 'yarn run watch:css' 'yarn run watch:js' 'yarn run watch:html'",
    "start:p": "yarn run build:clean; concurrently 'yarn run assets' 'yarn run server' 'yarn run build:css' 'yarn run build:js' 'yarn run build:html'",
    "server": "live-server ./public --port 3000 --quiet --watch ./public --watch ./src --wait 0",
    "watch:css": "cross-env NODE_ENV=development node ./internals/postcss -w",
    "watch:js": "cross-env NODE_ENV=development webpack --config webpack.config.js --colors --watch",
    "watch:html": "pug --watch ./src/templates/*.pug --obj ./config.js --pretty --out ./public",
    "build": "yarn run build:clean; mkdir ./public; yarn run assets; yarn run build:js; yarn run build:css; yarn run build:html;",
    "build:css": "cross-env NODE_ENV=production node ./internals/postcss",
    "build:js": "cross-env NODE_ENV=production webpack --config webpack.config.js --colors;",
    "build:html": "cross-env NODE_ENV=production pug ./src/templates/*.pug --obj ./config.js --out ./public",
    "build:clean": "rm -rf ./public",
    "assets": "mkdir public; cp -R ./assets/ ./public/assets; yarn run modernizr",
    "modernizr": "modernizr -c ./internals/modernizr.config.json -d ./public/assets/js/third-party/modernizr/modernizr.min.js -u",
    "scripts:three:examples": "shjs ./scripts/three-examples; yarn run prettier",
    "prettier": "prettier --write --single-quote --print-width 80 './src/js/**/*.js'",
    "prettier:internals": "prettier --write --single-quote --print-width 80 \"internals/**/*.js\"",
    "stylefmt": "stylefmt --c .stylelintrc -r \"src/**/**/*.css\"",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "formatting": "yarn run prettier && yarn run stylefmt",
    "lint-staged": "echo 'Running lint-staged for staged files...' && lint-staged",
    "lint:css": "stylelint ./src/**/*.css ./src/**/**/*.css --cache",
    "lint:eslint": "eslint --ext .js ./src ./internals --cache",
    "lint:fix": "eslint --ext .js ./src --cache --fix",
    "lint:js": "yarn run lint:eslint",
    "lint": "yarn run lint:js && yarn run lint:css",
    "precommit": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Stinkstudios/component-arkit-web.git"
  },
  "author": "Amelie Rosser <amelierosser1986@gmail.com> (https://www.ixviii.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stinkstudios/component-arkit-web/issues"
  },
  "homepage": "https://github.com/Stinkstudios/component-arkit-web",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "^0.4.0",
    "husky": "^0.14.2",
    "live-server": "^1.2.0",
    "modernizr": "^3.5.0",
    "postcss": "^6.0.5",
    "postcss-cli": "^4.1.0",
    "postcss-conditionals": "2.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-focus": "^2.0.0",
    "postcss-for": "2.1.1",
    "postcss-loader": "^2.0.6",
    "postcss-math": "^0.0.8",
    "postcss-mixins": "^6.0.1",
    "postcss-reporter": "^4.0.0",
    "postcss-scss": "^1.0.2",
    "prettier": "^1.5.2",
    "pug-cli": "^1.0.0-alpha6",
    "redux-saga": "^0.15.4",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "stylelint-order": "^0.6.0",
    "stylelint-scss": "^1.4.4",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "commander": "^2.11.0",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.3",
    "fastclick": "^1.0.6",
    "file-extension": "^3.1.2",
    "file-name": "^0.1.0",
    "happens": "^0.6.0",
    "lodash.find": "^4.6.0",
    "postcss-automath": "^1.0.1",
    "postcss-import": "^10.0.0",
    "postcss-nested": "^2.0.4",
    "postcss-simple-vars": "^4.0.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "shelljs": "^0.7.8",
    "stats-js": "^1.0.0-alpha1",
    "three": "^0.86.0",
    "ua-parser-js": "^0.7.14",
    "uuid": "^3.1.0"
  }
}
